#include<stdio.h>
int main()
{
    int n=0,a=0,b=0,c=0,d=0,k;
    scanf("%d",&n);
    for (int i = 0; i < n; i ++){
        scanf("%d",&k);
        if(k <= 18){a ++;}
        else if(k>= 19&&k < 36){b ++;}
        else if(k >= 36&&k< 61) {c ++;}
        else if(k >= 61) {d ++;}
    }
      printf("1-18: %0.2f%%\n",(double)a*100/n);
      printf("19-35: %0.2f%%\n",(double)b*100/n);
      printf("36-60: %0.2f%%\n",(double)c* 100/n);
      printf("61-: %0.2f%%\n",(double)d*100/n);
    return 0;
  }
